      <operator activated="true" class="python_scripting:python_transformer" compatibility="10.1.002" expanded="true" height="82" name="LORE" width="90" x="1318" y="442">
        <parameter key="editable" value="true"/>
        <parameter key="operator" value="{&#13;&#10;  &quot;name&quot;: &quot;Custom Python Transformer&quot;,&#13;&#10;  &quot;dropSpecial&quot;: false,&#13;&#10;  &quot;parameters&quot;: [&#13;&#10;    {&#13;&#10;      &quot;name&quot;: &quot;model_name&quot;,&#13;&#10;      &quot;type&quot;: &quot;string&quot;,&#13;&#10;      &quot;description&quot;: &quot;This should contain the reference to a valid prefix to be used to select a model/explainer&quot;,&#13;&#10;      &quot;optional&quot;: false&#13;&#10;    },&#13;&#10;    {&#13;&#10;      &quot;name&quot;: &quot;2nd_parameter&quot;,&#13;&#10;      &quot;type&quot;: &quot;integer&quot;,&#13;&#10;      &quot;description&quot;: &quot;This is an example of an mandatory integer parameter with a default value 100\.&quot;,&#13;&#10;      &quot;value&quot;: 100&#13;&#10;    },&#13;&#10;    {&#13;&#10;      &quot;name&quot;: &quot;3rd_parameter&quot;,&#13;&#10;      &quot;type&quot;: &quot;category&quot;,&#13;&#10;      &quot;description&quot;: &quot;An example of a categorical parameter\.&quot;,&#13;&#10;      &quot;categories&quot;: [&#13;&#10;        &quot;Category A&quot;,&#13;&#10;        &quot;Category B&quot;,&#13;&#10;        &quot;Category C&quot;,&#13;&#10;        &quot;Default Category&quot;&#13;&#10;      ],&#13;&#10;      &quot;value&quot;: &quot;Default Category&quot;&#13;&#10;    }&#13;&#10;  ],&#13;&#10;  &quot;inputs&quot;: [&#13;&#10;    {&#13;&#10;      &quot;name&quot;: &quot;data&quot;,&#13;&#10;      &quot;type&quot;: &quot;table&quot;&#13;&#10;    }&#10;  ],&#13;&#10;  &quot;outputs&quot;: [&#13;&#10;    {&#13;&#10;      &quot;name&quot;: &quot;out&quot;,&#13;&#10;      &quot;type&quot;: &quot;table&quot;&#13;&#10;    },&#13;&#10;    {&#13;&#10;      &quot;name&quot;: &quot;through&quot;,&#13;&#10;      &quot;type&quot;: &quot;table&quot;&#13;&#10;    }&#13;&#10;  ]&#13;&#10;}.from pandas import DataFrame&#13;&#10;import dill&#10;&#13;&#10;# Mandatory main function\. This example expects a single input followed by the&#13;&#10;# parameter dictionary\.&#13;&#10;def rm_main(data, parameters):&#13;&#10;&#9;&#10;&#9;print(f&quot;data: {data\.shape}&quot;)&#10;&#10;&#9;with open(f&quot;\./{parameters['model_name']}\.joblib&quot;, &quot;rb&quot;) as f:&#10;&#9;&#9;model = dill\.load(f)&#10;&#10;&#9;with open(f&quot;\./{parameters['model_name']}_explainer\.joblib&quot;, &quot;rb&quot;) as f:&#10;&#9;&#9;explainer = dill\.load(f)&#10;&#10;&#9;cols = [c for c in data\.columns if c != &quot;exhaustion&quot;]  # all columns except 'exhaustion'&#10;&#9;class_column = data[&quot;exhaustion&quot;]&#10;&#9;table = DataFrame(data[cols]\.values, columns=cols)  # put 'exhaustion' at the end&#10;&#9;table[&quot;exhaustion&quot;] = class_column&#10;&#10;&#9;if model:&#10;&#9;&#9;print(&quot;Classifing the instances&quot;)&#10;&#9;&#9;prediction = model\.predict(table[cols]\.values)&#10;&#9;&#9;table[&quot;prediction&quot;] = prediction&#10;&#9;&#9;if explainer:&#10;&#9;&#9;&#9;print(&quot;Creating the explanation\.\.\.&quot;)&#10;&#9;&#9;&#9;explanation = explainer\.explain(table[cols]\.values[0])&#10;&#9;&#9;&#9;print(explanation)&#10;&#9;&#9;&#9;&#10;&#9;&#9;&#9;&#10;&#9;&#10;&#10;&#13;&#10;&#9;# Return the new data frame and pass through the input data\.&#13;&#10;&#9;return table, data&#13;&#10;"/>
